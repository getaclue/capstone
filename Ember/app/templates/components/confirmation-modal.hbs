{{!-- Modal that will be used to confirm if user wants to procceed with operation --}}
{{#if (eq type "service")}}
  <h3>Delete the following {{type}}?</h3>
  <h4 style="text-align: center">{{model.name}}</h4>
{{else}}
  <h3>Delete the following {{type}}?</h3>
  <h4 style="text-align: center">{{model.fullName}}</h4>
{{/if}}